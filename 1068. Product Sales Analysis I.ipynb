    "import pandas as pd\n",
    "\n",
    "def sales_analysis(sales: pd.DataFrame, product: pd.DataFrame) -> pd.DataFrame:\n",
    "    merge_df = pd.merge(sales,product,on='product_id')\n",
    "\n",
    "    result = merge_df[['product_name', 'year','price']]\n",
    "    return result"
   ]
