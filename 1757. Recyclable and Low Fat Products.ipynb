    "import pandas as pd\n",
    "\n",
    "def find_products(products: pd.DataFrame) -> pd.DataFrame:\n",
    "    filt = products.loc[(products[\"low_fats\"]==\"Y\") & (products[\"recyclable\"]==\"Y\"),[\"product_id\"]]\n",
    "    return filt"
