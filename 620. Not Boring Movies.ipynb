    "import pandas as pd\n",
    "\n",
    "def not_boring_movies(cinema: pd.DataFrame) -> pd.DataFrame:\n",
    "    cinema_filt = cinema[(cinema['description'] != 'boring')  & ( cinema['id']%2 !=0)]\n",
    "    cinema_filt = cinema_filt.sort_values(by='rating',ascending=False)\n",
    "    return cinema_filt\n",
    "    "
