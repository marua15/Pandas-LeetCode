    "import pandas as pd\n",
    "\n",
    "def duplicate_emails(person: pd.DataFrame) -> pd.DataFrame:\n",
    "    find_duplicate = person[person.duplicated(subset='email',keep='first')][['email']].drop_duplicates()\n",
    "    return find_duplicate"
   ]
